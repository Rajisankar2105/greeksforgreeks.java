
class Solution {
    public boolean isSubset(int a[], int b[]) {
        if(b.length==0)return true;
        if(a.length<b.length)return false;
        Arrays.sort(a);
        Arrays.sort(b);
        int j=0;
        for(int  i=0;i<a.length&&j<b.length;i++){
            if(a[i]==b[j]){
                j++;
            }
        }
        return j==b.length;
    }
}
